{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run env:dev && node server.js --dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "lint": "eslint src private webpack.config.js --ext js,jsx",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "webpack --config webpack.prod.js",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true"
  },
  "author": "Sergiu Miclea",
  "license": "AGPL-3.0",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.1.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.84.0",
    "flow-typed": "^2.5.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0-beta.4",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "mobx": "^5.5.2",
    "mobx-react": "^5.3.6",
    "moment": "^2.22.2",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.0.0",
    "react-modal": "^3.6.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "styled-components": "^4.0.2",
    "styled-tools": "^1.5.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  }
}
